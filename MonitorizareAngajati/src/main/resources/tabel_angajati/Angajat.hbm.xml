<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="domain.Angajat"
           table="angajati">

        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property
                name="nume"
                column="nume"/>

        <property
                name="nume_de_utilizator"
                column="nume_de_utilizator"/>

        <property
                name="parola"
                column="parola"/>

        <property
                name="ora_conectare"
                column="ora_conectare"/>

        <property
                name="ora_deconectare"
                column="ora_deconectare"/>

    </class>
</hibernate-mapping>